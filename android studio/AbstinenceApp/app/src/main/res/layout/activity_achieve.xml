            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />
            <View style="@style/achieveFrameStyle" />

        </LinearLayout>
    </ScrollView>
    <include layout="@layout/layout_back_button"/>
</androidx.constraintlayout.widget.ConstraintLayout>
