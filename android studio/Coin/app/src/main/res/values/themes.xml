<resources>

    <style name="Base.Theme.Coin" parent="Theme.Material3.Dark.NoActionBar">
        <item name="colorSurface">@color/gray_990</item>
        <item name="colorSurfaceContainer">@color/gray_950</item>
        <item name="colorOnSurface">@color/gray_200</item>

    </style>

    <style name="Theme.Coin" parent="Base.Theme.Coin">
        <item name="android:statusBarColor">?attr/colorSurface</item>
        <item name="android:navigationBarColor">?attr/colorSurfaceContainer</item>
        <item name="android:fontFamily">@font/open_sans_600</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

</resources>



